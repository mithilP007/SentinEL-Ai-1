# SentinEL AI Environment Variables
# Copy this file to .env and fill in your API keys

# Google Gemini API Key (Primary LLM - Gemini 2.0 Flash)
GEMINI_API_KEY=your_gemini_api_key_here

# Groq API Key (Fallback 1 - Llama 3.3 70B)
GROQ_API_KEY=your_groq_api_key_here

# Mistral API Key (Fallback 2)
MISTRAL_API_KEY=your_mistral_api_key_here

# OpenWeather API Key (Weather data)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# AIS Stream API Key (Ship tracking)
AISSTREAM_API_KEY=your_aisstream_api_key_here

# OpenAI API Key (Vector embeddings - optional)
OPENAI_API_KEY=your_openai_api_key_here
